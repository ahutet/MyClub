<tr id="tr_{{ team.id }}">
    <td class="align-middle">
        <a href="{{ path('front_team_view', {team: team.id}) }}"
           class="font-weight-bold text-white"><u>{{ team.name }}</u></a>
    </td>
    <td class="align-middle text-white">{{ team.division }}</td>
    <td class="align-middle text-white">{{ team.season.name }}</td>
</tr>